<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="UnusedAttribute">

    <font
        android:font="@font/font_light"
        android:fontStyle="normal"
        android:fontWeight="100"
        app:font="@font/font_light"
        app:fontStyle="normal"
        app:fontWeight="100" />

    <font
        android:font="@font/font_normal"
        android:fontStyle="normal"
        android:fontWeight="200"
        app:font="@font/font_normal"
        app:fontStyle="normal"
        app:fontWeight="200" />

    <font
        android:font="@font/font_medium"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/font_medium"
        app:fontStyle="normal"
        app:fontWeight="400" />

    <font
        android:font="@font/font_bold"
        android:fontStyle="normal"
        android:fontWeight="700"
        app:font="@font/font_bold"
        app:fontStyle="normal"
        app:fontWeight="700" />

</font-family>